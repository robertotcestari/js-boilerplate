{
    "parser": "babel-eslint",
    "env": {
        "jest": true,
        "browser": true
    },
    "extends": [
        "standard",
        "plugin:prettier/recommended"
    ],
    "rules": {
        "space-before-function-paren": "off",
        "comma-dangle": "off",
        "prettier/prettier": "error"
    },
    "plugins": [
        "prettier"
    ]
}